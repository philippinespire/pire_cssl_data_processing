#!/bin/bash
#SBATCH --job-name=MitoZ_example-%j
#SBATCH --output=MitoZ_example-%j.out
#SBATCH -c 8

enable_lmod
module load container_env MitoZ

export SINGULARITY_BIND=/home/e1garcia

crun MitoZ.py all2 --genetic_code 2 --clade Chordata --outprefix Ssp-CGub_041-test --thread_number 16 --fastq1 Ssp-CGub_041-Ex1-cssl.clmp.fp2_r1.fq.gz --fastq2 Ssp-CGub_041-Ex1-cssl.clmp.fp2_r2.fq.gz --fastq_read_length 150
