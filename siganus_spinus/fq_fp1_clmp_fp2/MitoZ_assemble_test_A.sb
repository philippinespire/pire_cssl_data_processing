#!/bin/bash
#SBATCH --job-name=MitoZ_example-%j
#SBATCH --output=MitoZ_example-%j.out
#SBATCH -c 10

enable_lmod
module load container_env MitoZ

export SINGULARITY_BIND=/home/e1garcia

mkdir /home/e1garcia/shotgun_PIRE/pire_cssl_data_processing/siganus_spinus/fq_fp1_clmp_fp2/Ssp-AAtu_009_MitoZ
cd /home/e1garcia/shotgun_PIRE/pire_cssl_data_processing/siganus_spinus/fq_fp1_clmp_fp2/Ssp-AAtu_009_MitoZ

crun MitoZ.py all2 --genetic_code 2 --clade Chordata --outprefix Ssp-AAtu_009 --thread_number 10 --fastq1 /home/e1garcia/shotgun_PIRE/pire_cssl_data_processing/siganus_spinus/fq_fp1_clmp_fp2/Ssp-AAtu_009-Ex1-cssl.clmp.fp2_r1.fq.gz --fastq2 /home/e1garcia/shotgun_PIRE/pire_cssl_data_processing/siganus_spinus/fq_fp1_clmp_fp2/Ssp-AAtu_009-Ex1-cssl.clmp.fp2_r2.fq.gz --fastq_read_length 150
